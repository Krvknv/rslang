{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier", "import", "@typescript-eslint"],
    "rules": {
        "@typescript-eslint/no-explicit-any": 2,
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "import/no-duplicates": 0,
        "import/prefer-default-export": "off",
        "no-restricted-syntax": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "no-param-reassign": 0,
        "consistent-return": 0,
        "no-return-assign": 0
    }
}
